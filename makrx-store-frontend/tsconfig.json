{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "paths": {
      "@/*": ["makrx-store-frontend/src/*"],
      "@/components/*": ["makrx-store-frontend/src/components/*"],
      "@/lib/*": ["makrx-store-frontend/src/lib/*"],
      "@/types/*": ["makrx-store-frontend/src/types/*"],
      "@/store/*": ["makrx-store-frontend/src/store/*"],
      "@/utils/*": ["makrx-store-frontend/src/utils/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
