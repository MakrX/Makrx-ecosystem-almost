# MakrX Store Frontend Production Environment
# Copy this file to .env.production and update with your production values

# WebSocket URL for the real-time event service
NEXT_PUBLIC_EVENT_SERVICE_URL=wss://events.yourdomain.com

# Base URL of the Keycloak authentication server
NEXT_PUBLIC_KEYCLOAK_URL=https://auth.yourdomain.com

# Keycloak realm used for authentication
NEXT_PUBLIC_KEYCLOAK_REALM=makrx

# Keycloak client ID registered for this frontend
NEXT_PUBLIC_KEYCLOAK_CLIENT_ID=makrx-store-frontend

# Base REST API URL for the store backend
NEXT_PUBLIC_API_URL=https://api.yourdomain.com

# WebSocket URL for store backend communication
NEXT_PUBLIC_WS_URL=wss://ws.yourdomain.com
