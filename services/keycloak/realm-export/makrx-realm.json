{
  "realm": "makrx",
  "displayName": "MakrX Ecosystem",
  "displayNameHtml": "<strong>Makr</strong><span style=\"color: #FFC107;\">X</span> Ecosystem",
  "enabled": true,
  "registrationAllowed": true,
  "registrationEmailAsUsername": true,
  "rememberMe": true,
  "verifyEmail": true,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,
  "resetPasswordAllowed": true,
  "editUsernameAllowed": false,
  "bruteForceProtected": true,
  "permanentLockout": false,
  "maxFailureWaitSeconds": 900,
  "minimumQuickLoginWaitSeconds": 60,
  "waitIncrementSeconds": 60,
  "quickLoginCheckMilliSeconds": 1000,
  "maxDeltaTimeSeconds": 43200,
  "failureFactor": 5,
  "passwordPolicy": "length(8) and lowerCase(1) and upperCase(1) and digits(1) and specialChars(1)",
  "loginTheme": "makrx-login",
  "adminTheme": "keycloak",
  "emailTheme": "makrx-login",
  "accountTheme": "keycloak.v3",
  "internationalizationEnabled": true,
  "supportedLocales": [
    "en"
  ],
  "defaultLocale": "en",
  "accessTokenLifespan": 300,
  "accessTokenLifespanForImplicitFlow": 300,
  "refreshTokenLifespan": 1800,
  "refreshTokenLifespanRememberMe": 2592000,
  "ssoSessionIdleTimeout": 1800,
  "ssoSessionMaxLifespan": 28800,
  "ssoSessionIdleTimeoutRememberMe": 2592000,
  "ssoSessionMaxLifespanRememberMe": 2592000,
  "offlineSessionIdleTimeout": 2592000,
  "offlineSessionMaxLifespan": 0,
  "smtpServer": {
    "host": "smtp.makrx.org",
    "port": "587",
    "auth": "true",
    "starttls": "true",
    "ssl": "false",
    "user": "no-reply@makrx.org",
    "password": "${SMTP_PASSWORD}",
    "from": "no-reply@makrx.org",
    "fromDisplayName": "MakrX"
  },
  "clients": [
    {
      "clientId": "makrx-gateway-frontend",
      "name": "MakrX Gateway Frontend",
      "description": "Main MakrX gateway application",
      "enabled": true,
      "publicClient": true,
      "redirectUris": [
        "http://localhost:5173/*",
        "http://localhost:3000/*",
        "https://makrx.org/*"
      ],
      "webOrigins": [
        "http://localhost:5173",
        "http://localhost:3000",
        "https://makrx.org"
      ],
      "protocol": "openid-connect",
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "attributes": {
        "pkce.code.challenge.method": "S256"
      },
      "protocolMappers": [
        {
          "name": "realm roles",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-realm-role-mapper",
          "consentRequired": false,
          "config": {
            "multivalued": "true",
            "user.attribute": "",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "userinfo.token.claim": "true",
            "claim.name": "realm_access.roles",
            "jsonType.label": "String"
          }
        }
      ]
    },
    {
      "clientId": "makrcave-frontend",
      "name": "MakrCave Frontend",
      "description": "Makerspace management portal",
      "enabled": true,
      "publicClient": true,
      "redirectUris": [
        "http://localhost:5174/*",
        "https://cave.makrx.org/*"
      ],
      "webOrigins": [
        "http://localhost:5174",
        "https://cave.makrx.org"
      ],
      "protocol": "openid-connect",
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "attributes": {
        "pkce.code.challenge.method": "S256"
      },
      "protocolMappers": [
        {
          "name": "realm roles",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-realm-role-mapper",
          "consentRequired": false,
          "config": {
            "multivalued": "true",
            "user.attribute": "",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "userinfo.token.claim": "true",
            "claim.name": "realm_access.roles",
            "jsonType.label": "String"
          }
        }
      ]
    },
    {
      "clientId": "makrx-store-frontend",
      "name": "MakrX Store Frontend",
      "description": "Ecommerce and 3D printing platform",
      "enabled": true,
      "publicClient": true,
      "redirectUris": [
        "http://localhost:5175/*",
        "https://store.makrx.org/*"
      ],
      "webOrigins": [
        "http://localhost:5175",
        "https://store.makrx.org"
      ],
      "protocol": "openid-connect",
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "attributes": {
        "pkce.code.challenge.method": "S256"
      },
      "protocolMappers": [
        {
          "name": "realm roles",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-realm-role-mapper",
          "consentRequired": false,
          "config": {
            "multivalued": "true",
            "user.attribute": "",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "userinfo.token.claim": "true",
            "claim.name": "realm_access.roles",
            "jsonType.label": "String"
          }
        }
      ]
    },
    {
      "clientId": "makrcave-backend",
      "name": "MakrCave Backend",
      "description": "Backend API for makerspace management",
      "enabled": true,
      "serviceAccountsEnabled": true,
      "publicClient": false,
      "protocol": "openid-connect",
      "secret": "CHANGE_ME",
      "defaultClientScopes": [
        "email",
        "profile",
        "roles",
        "makrcave-backend-scope"
      ]
    },
    {
      "clientId": "makrx-store-backend",
      "name": "MakrX Store Backend",
      "description": "Backend API for ecommerce platform",
      "enabled": true,
      "serviceAccountsEnabled": true,
      "publicClient": false,
      "protocol": "openid-connect",
      "secret": "CHANGE_ME",
      "defaultClientScopes": [
        "email",
        "profile",
        "roles",
        "makrx-store-backend-scope"
      ]
    },
    {
      "clientId": "auth-service",
      "name": "Auth Service Backend",
      "description": "Authentication microservice",
      "enabled": true,
      "serviceAccountsEnabled": true,
      "publicClient": false,
      "protocol": "openid-connect",
      "secret": "CHANGE_ME"
    }
  ],
  "clientScopes": [
    {
      "name": "makrcave-backend-scope",
      "description": "Service scope for MakrCave backend",
      "protocol": "openid-connect",
      "attributes": {
        "include.in.token.scope": "true",
        "display.on.consent.screen": "false"
      },
      "protocolMappers": [
        {
          "name": "makrcave-backend-audience",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-audience-mapper",
          "consentRequired": false,
          "config": {
            "included.client.audience": "makrcave-backend",
            "id.token.claim": "false",
            "access.token.claim": "true"
          }
        }
      ],
      "composite": true,
      "composites": {
        "realm": [
          "service_provider"
        ]
      }
    },
    {
      "name": "makrx-store-backend-scope",
      "description": "Service scope for MakrX Store backend",
      "protocol": "openid-connect",
      "attributes": {
        "include.in.token.scope": "true",
        "display.on.consent.screen": "false"
      },
      "protocolMappers": [
        {
          "name": "makrx-store-backend-audience",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-audience-mapper",
          "consentRequired": false,
          "config": {
            "included.client.audience": "makrx-store-backend",
            "id.token.claim": "false",
            "access.token.claim": "true"
          }
        }
      ],
      "composite": true,
      "composites": {
        "realm": [
          "service_provider"
        ]
      }
    }
  ],
  "roles": {
    "realm": [
      {
        "name": "super_admin",
        "description": "Super administrator with full system access"
      },
      {
        "name": "admin",
        "description": "Administrator with elevated permissions"
      },
      {
        "name": "makerspace_admin",
        "description": "Makerspace administrator"
      },
      {
        "name": "user",
        "description": "Standard user"
      },
      {
        "name": "service_provider",
        "description": "Service provider for manufacturing and fulfillment"
      }
    ]
  },
  "users": [
    {
      "username": "admin",
      "email": "admin@makrx.org",
      "firstName": "MakrX",
      "lastName": "Admin",
      "enabled": true,
      "emailVerified": true,
      "credentials": [
        {
          "type": "password",
          "value": "CHANGE_ME",
          "temporary": false
        }
      ],
      "realmRoles": [
        "super_admin",
        "admin"
      ]
    },
    {
      "username": "demo",
      "email": "demo@makrx.org",
      "firstName": "Demo",
      "lastName": "User",
      "enabled": true,
      "emailVerified": true,
      "credentials": [
        {
          "type": "password",
          "value": "CHANGE_ME",
          "temporary": false
        }
      ],
      "realmRoles": [
        "user"
      ]
    }
  ],
  "groups": [
    {
      "name": "Administrators",
      "path": "/Administrators",
      "realmRoles": [
        "admin",
        "super_admin"
      ]
    },
    {
      "name": "Service Providers",
      "path": "/Service Providers",
      "realmRoles": [
        "service_provider"
      ]
    }
  ]
}
