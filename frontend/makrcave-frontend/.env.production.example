# MakrCave Frontend Production Environment
# Copy this file to .env.production and update with your production values

# Base URL of the Keycloak server
VITE_KEYCLOAK_URL=https://auth.yourdomain.com

# Keycloak realm used for authentication
VITE_KEYCLOAK_REALM=makrx

# OAuth client ID registered for the MakrCave frontend
VITE_KEYCLOAK_CLIENT_ID=makrcave-frontend

# Base URL for the MakrCave backend API
VITE_API_BASE_URL=https://api.yourdomain.com
